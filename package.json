{
  "name": "go-ahead",
  "version": "1.0.0",
  "description": "Frontend form validation",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "watch": "./node_modules/.bin/webpack --config webpack.config.js --progress --hot",
    "test": "./node_modules/.bin/mocha --require babel-polyfill --compilers js:babel-register --reporter=nyan",
    "test-watch": "./node_modules/.bin/mocha --require babel-polyfill --compilers js:babel-register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitch-rickman/go-ahead.git"
  },
  "keywords": [
    "Form",
    "Validation"
  ],
